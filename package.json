{
  "name": "gls-tracking-app",
  "version": "1.1.0",
  "description": "GLS Package Tracking Application with React frontend and Node.js backend",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "production": "concurrently \"npm run server\" \"npm run serve-build\"",
    "serve-build": "cd client && serve -s build -l 3000",
    "install-deps": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start"
  },
  "keywords": ["gls", "tracking", "react", "nodejs", "express", "postgresql"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
